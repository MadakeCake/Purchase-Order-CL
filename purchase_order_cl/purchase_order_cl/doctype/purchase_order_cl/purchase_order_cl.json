      "read_only": 1
    },
    {
      "fieldname": "total",
      "label": "Total",
      "fieldtype": "Currency",
      "read_only": 1
    },
    {
      "fieldname": "observaciones",
      "label": "Observaciones",
      "fieldtype": "Text"
    }
